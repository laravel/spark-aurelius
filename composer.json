{
    "name": "laravel/spark-aurelius",
    "description": "Laravel Spark provides scaffolding for Laravel SaaS applications.",
    "keywords": ["laravel", "stripe", "billing", "scaffolding", "saas"],
    "license": "MIT",
    "authors": [
        {
            "name": "Taylor Otwell",
            "email": "taylor@laravel.com"
        }
    ],
    "require": {
        "php": "^7.2|^8.0",
        "ext-json": "*",
        "ext-zip": "*",
        "erusev/parsedown": "~1.0",
        "firebase/php-jwt": "~4.0|~5.0",
        "intervention/image": "^2.3",
        "laravel/cashier": "^12.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.0",
        "mpociot/vat-calculator": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "Laravel\\Spark\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "11.x-dev"
        }
    },
    "minimum-stability": "dev"
}
